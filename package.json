{
  "name": "react-javelin",
  "version": "1.1.8",
  "description": "Handy react helpers for @javelin/ecs",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "types": " tsc --emitDeclarationOnly --outDir dist",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "tsc -b && tsc -b tsconfig.cjs.json",
    "test": "jest",
    "lint": "eslint \"./src/**/*.{ts,tsx,js,jsx}\"",
    "prettier": "prettier --check \"./src/**/*.{ts,tsx,js,jsx}\"",
    "commit": "commit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kenjinp/react-javelin.git"
  },
  "keywords": [
    "react",
    "web",
    "develoment",
    "games",
    "ecs",
    "typescript"
  ],
  "author": "Kenneth Pirman <github@kenny.wtf>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kenjinp/react-javelin/issues"
  },
  "homepage": "https://github.com/kenjinp/react-javelin#readme",
  "peerDependencies": {
    "@javelin/ecs": "^1.0.0-alpha.12",
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@commitlint/prompt-cli": "^13.1.0",
    "@javelin/ecs": "^1.0.0-alpha.12",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.2",
    "@semantic-release/npm": "^8.0.3",
    "@semantic-release/release-notes-generator": "^10.0.2",
    "@trivago/prettier-plugin-sort-imports": "^2.0.4",
    "@types/react": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-scope": "^6.0.0",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.3"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/npm",
        {
          "assets": [
            "lib",
            "package.json"
          ]
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "package-lock.json",
            "CHANGELOG.md"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      [
        "@semantic-release/github",
        {
          "assets": [
            [
              "dist"
            ]
          ]
        }
      ]
    ]
  },
  "dependencies": {
    "lodash": "^4.17.21"
  }
}
